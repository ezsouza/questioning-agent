{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "ignorePatterns": [
    "lib/generated/**",
    "node_modules/**",
    ".next/**",
    "out/**"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "@typescript-eslint/no-explicit-any": "warn"
  }
}
